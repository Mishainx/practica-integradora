<div id="listProductsContainer" class="listProductsContainer">
<div id="pageProducts">
    {{#each productsList.docs}}
    <p>Nombre: {{this.title}}</p>
    <p id="productViewId">Id: {{this.id}}</p>
    <p>Id: {{this.description}}</p>
    <p>Precio {{this.price}}</p>
    <p>Stock: {{this.stock}}</p>
    <p>code: {{this.code}}</p>
    <input id="productQuantity" type="number" placeholder="Indique cantidad solicitada">
    <button id="productQuantityButton">Enviar</button>
    {{/each}}
</div>
<div>
    <button>Previus page</button>
    <input type="number" value={{productsList.pagingCounter}}>
    <button><a href="./products?page={{productsList.nextPage}}" >Next Page</a></button>
</div>
    
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/products.js"></script>